{
  "type": "timeseries",
  "title": true,
  "title_text": "Hits / Misses",
  "width": 39,
  "height": 13,
  "x": 0,
  "y": {{position}},
  "tile_def": {
    "viz": "timeseries",
    "requests": [
      {
        "q": "avg:aws.elasticache.cache_hits{cacheclusterid:{{~toLowerCase name~}}}.as_count()",
        "style": {
          "width": "normal",
          "palette": "dog_classic",
          "type": "solid"
        },
        "aggregator": "avg",
        "type": "line",
        "metadata": {
          "avg:aws.elasticache.cache_hits{cacheclusterid:{{~toLowerCase name~}}}.as_count()": {
            "alias": "Hits"
          }
        }
      },
      {
        "q": "avg:aws.elasticache.cache_misses{cacheclusterid:{{~toLowerCase name~}}}.as_count()",
        "style": {
          "width": "normal",
          "palette": "dog_classic",
          "type": "solid"
        },
        "aggregator": "avg",
        "type": "line",
        "metadata": {
          "avg:aws.elasticache.cache_misses{cacheclusterid:{{~toLowerCase name~}}}.as_count()": {
            "alias": "Misses"
          }
        }
      }
    ],
    "autoscale": true
  }
}
