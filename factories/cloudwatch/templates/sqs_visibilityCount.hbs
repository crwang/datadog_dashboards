{
  "type": "query_value",
  "title": true,
  "title_text": "Currently Visible",
  "width": 21,
  "height": 7,
  "x": 0,
  "y": {{position}},
  "tile_def": {
    "viz": "query_value",
    "requests": [
      {
        "q": "avg:aws.sqs.approximate_number_of_messages_visible{{{querify query}}}",
        {{#if queue.conditional_formats}}
        "conditional_formats": {{JSONStringify queue.conditional_formats}},
        {{/if}}
        "aggregator": "last"
      }
    ],
    "autoscale": true,
    "precision": 2,
    "custom_unit": "m"
  }
}
