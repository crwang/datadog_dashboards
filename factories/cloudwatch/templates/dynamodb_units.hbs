{
  "type": "timeseries",
  "title": true,
  "title_text": "{{mode}}",
  "width": {{width}},
  "height": {{height}},
  "x": {{x}},
  "y": {{y}},
  "tile_def": {
    "viz": "timeseries",
    "requests": [
      {
        "q": "max:aws.dynamodb.consumed_{{toLowerCase mode}}_capacity_units{{{querify query}}}",
        "style": {
          "width": "normal",
          "palette": "dog_classic",
          "type": "solid"
        },
        "aggregator": "avg",
        "type": "area",
        "metadata": {
          "max:aws.dynamodb.consumed_{{toLowerCase mode}}_capacity_units{{{querify query}}}": {
            "alias": "Consumed"
          }
        }
      },
      {
        "q": "max:aws.dynamodb.provisioned_{{toLowerCase mode}}_capacity_units{{{querify query}}}",
        "style": {
          "width": "thin",
          "palette": "orange",
          "type": "solid"
        },
        "type": "line",
        "metadata": {
          "max:aws.dynamodb.provisioned_{{toLowerCase mode}}_capacity_units{{{querify query}}}": {
            "alias": "Provisioned"
          }
        }
      }
    ],
    "autoscale": true
  }
}
