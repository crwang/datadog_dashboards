{
  "requests": [
  {{#each queries}}
    {
      {{#if type}}
      "type": "{{type}}",
      {{else}}
      "type": "line",
      {{/if}}
      {{#style}}
      "style":  {
        "palette": "{{palette}}",
        "type": "{{type}}",
        "width": "{{width}}"
      },
      {{else}}
      "style": {
        "palette": "dog_classic",
        "type": "solid",
        "width": "normal"
      },
      {{/style}}
      {{#if conditional_formats}}
      "conditional_formats": [
        {{#each conditional_formats}}
        {
          "palette": "{{palette}}",
          "value": "{{{value}}}",
          "comparator": "{{{comparator}}}"
        }{{#unless @last}},{{/unless}}
        {{/each}}
      ],
      {{else}}
      "conditional_formats": [],
      {{/if}}
      {{#if aggregator}}
      "aggregator": "{{aggregator}}",
      {{else}}
      "aggregator": "avg",
      {{/if}}
      {{#if qs}}
        "metadata": {
          {{#each qs}}
            {{#if alias}}
              "{{{q}}}": {
                "alias": "{{alias}}"
              }{{#unless @last}},{{/unless}}
            {{/if}}
          {{/each}}
        },
        "q": "{{#each qs}}{{{q}}}{{#unless @last}},{{/unless}}{{/each}}"
      {{else}}
        {{#if alias}}
        "metadata": {
          "{{{q}}}": {
            "alias": "{{alias}}"
          }
        },
        {{/if}}
        "q": "{{{q}}}"
      {{/if}}
    }
    {{#unless @last}},{{/unless}}
  {{/each}}
  ],
  "viz": "{{viz}}",
  {{#if custom_unit}}
  "custom_unit": "{{custom_unit}}",
  {{/if}}
  {{#if precision}}
  "precision": "{{precision}}",
  {{/if}}
  {{#if markers}}
  "markers": [
    {{#each markers}}
    {
      "dim": "{{dim}}",
      "type": "{{type}}",
      "value": "{{{value}}}",
      "val": {{val}},
      "label": "{{label}}"
    }{{#unless @last}},{{/unless}}
    {{/each}}
  ],
  {{/if}}
  {{#yaxis}}
  "yaxis": {
    "max": "{{max}}",
    "min": "{{min}}"
  },
  {{/yaxis}}

  "autoscale": true,
  "xaxis": {},
  "status": "done"
}
